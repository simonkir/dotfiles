# -*- mode: snippet -*-
# name: plot (using gnuplot)
# key: pl
# --
#+name: `(format-time-string "%Y%m%d-%H%M%S")`
#+begin_src gnuplot :file /tmp/gnuplot-`(format-time-string "%Y%m%d-%H%M%S")`.png
load "~/.gnuplot"
set xlabel "$1 [$2]"
set xrange [0:$3]
set xtics $4
set ylabel "$5 [$6]"
set ytics $7
plot $8 title ""$0
#+end_src