# -*- mode: snippet -*-
# name: plot (using gnuplot)
# key: pl
# --
#+name: `(format-time-string "%Y%m%d-%H%M")`
#+begin_src gnuplot :file /tmp/gnuplot-`(format-time-string "%Y%m%d-%H%M")`.png
load "~/.gnuplot"
set xlabel "$1 [$2]"
set xtics $3
set ylabel "$4 [$5]"
set ytics $6
plot $7 title ""$0
#+end_src